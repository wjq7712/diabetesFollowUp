<script type="text/javascript">
    function mapToDatabase(viewStr) {
       //a：view b: database
        var obj = [{ a: "IGR", b: "FirstDetectedIGR" }, { a: "Therapeutic condition", b: "FirstDetectedTherapeuticCondition" }, { a: "DM_type", b: "DiagnosisDMDMtype" }, { a: "Family_history", b: "DiagnosisDMFamilyHistory" }, { a: "Assessment", b: "DiettherapyAssessment" }, { a: "Staple_food_per", b: "DiettherapyStapleFoodPerDay" }, { a: "vegetable_per_day", b: "DiettherapyVegetablePerDay"}];
         var databaseStr;
         for(j=0;j<obj.length;j++){
             if (obj[j].a == viewStr)
            {
                databaseStr=obj[j].b;
            }
        }
        return databaseStr;
    }
    function sumbit_info() {
         var data = [];
        // var sqlItems = new Array("FirstDetectedIGR", "FirstDetectedTherapeuticCondition", "DiagnosisDMDMtype", "DiagnosisDMFamilyHistory", "DiagnosisiDMFamilyHistoryFlag", "DiettherapyAssessment", "DiettherapyStapleFoodPerDay", "DiettherapyVegetablePerDay");
         //var viewItems = new Array("IGR", "Therapeutic_condition", "DM_type", "Family_history", "Family_history_flag", "Assessment", "Staple_food_per day", "vegetable_per_day");
         //var mapping3 = { IGR: "FirstDetectedIGR", Therapeutic_condition: "FirstDetectedTherapeuticCondition", DM_type: "DiagnosisDMDMtype", Family_history: "DiagnosisDMFamilyHistory", Assessment: "DiettherapyAssessment", Staple_food_per_day: "DiettherapyStapleFoodPerDay", vegetable_per_day: "DiettherapyVegetablePerDay"};

        $("#first input").each(function (index, element) {
            var id = $(this).attr("id");
            if (id) {
                var ids = new Array();
                ids = id.split("/");
                //下划线替换全部的空格
                var newId = ids[0].replace(/( )/g, "_");
                var position = mapToDatabase(newId);
                if (position) {
                    var value = $(this).val();
                    data.push({ id: position, value: value });
                }
            }
        })
        
        var sqlStr = ""
        var items=data[0].id;
        var value = "\'" + data[0].value + "\'";
        if(data){
            for(var i=1;i<data.length;i++){
                items+=","+data[i].id;
                value+=","+"\'"+data[i].value+"\'";
            }
        }
        sqlStr = "INSERT INTO diatbetes_history (" + items + ")" + " VALUES (" + value + ")";
        $.ajax({
            url: '@Url.Action("Save","Home")',
            type: "POST",
            data: { "sqlStr": sqlStr },
            success: function (data) {
                if (data.OK) { 
                    alert("success");
                } else { 
                alert(data.Message)}
            }
        })
   }


   String.prototype.format = function(args) {
    var result = this;
    if (arguments.length > 0) {    
        if (arguments.length == 1 && typeof (args) == "object") {
            for (var key in args) {
                if(args[key]!=undefined){
                    var reg = new RegExp("({" + key + "})", "g");
                    result = result.replace(reg, args[key]);
                }
            }
        }
        else {
            for (var i = 0; i < arguments.length; i++) {
                if (arguments[i] != undefined) {
                    //var reg = new RegExp("({[" + i + "]})", "g");//这个在索引大于9时会有问题，谢谢何以笙箫的指出
　　　　　　　　　　　　var reg= new RegExp("({)" + i + "(})", "g");
                    result = result.replace(reg, arguments[i]);
                }
            }
        }
    }
    return result;
}
</script>